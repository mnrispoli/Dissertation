isbn = {0028-0836},
issn = {14764687},
journal = {Nature},
number = {7197},
pages = {891--894},
pmid = {18548065},
title = {{Direct observation of Anderson localization of matter waves in a controlled disorder}},
volume = {453},
year = {2008}
}
