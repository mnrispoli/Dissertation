issn = {10797114},
journal = {Physical Review Letters},
number = {26},
pages = {1--6},
title = {{Observation of Slow Dynamics near the Many-Body Localization Transition in One-Dimensional Quasiperiodic Systems}},
volume = {119},
year = {2017}
}
