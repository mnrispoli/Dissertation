file = {:Users/matthewrispoli/Library/Application Support/Mendeley Desktop/Downloaded/Luitz, Huveneers, De Roeck - 2017 - How a Small Quantum Bath Can Thermalize Long Localized Chains.pdf:pdf},
issn = {10797114},
journal = {Physical Review Letters},
