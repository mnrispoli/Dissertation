@article{Gornyi2005,
abstract = {We study transport of interacting electrons in a low-dimensional disordered system at low temperature {\$}T{\$}. In view of localization by disorder, the conductivity {\$}\backslashsigma(T){\$} may only be non-zero due to electron-electron scattering. For weak interactions, the weak-localization regime crosses over with lowering {\$}T{\$} into a dephasing-induced "power-law hopping". As {\$}T{\$} is further decreased, the Anderson localization in Fock space crucially affects {\$}\backslashsigma(T){\$}, inducing a transition at {\$}T=T{\_}c{\$}, so that {\$}\backslashsigma(T{\textless}T{\_}c)=0{\$}. The critical behavior of {\$}\backslashsigma(T){\$} above {\$}T{\_}c{\$} is {\$}\backslashln\backslashsigma(T)\backslashpropto - (T-T{\_}c){\^{}}{\{}-1/2{\}}{\$}. The mechanism of transport in the critical regime is many-particle transitions between distant states in Fock space.},
